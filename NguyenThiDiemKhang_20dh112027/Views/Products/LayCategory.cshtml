@using NguyenThiDiemKhang_20dh112027.Models
@model IEnumerable<Category>

<div class="title">NHÀ XUẤT BẢN</div>
<ul>
    @foreach (var item in Model)
    {
        <a href="@Url.Action("SPTheoCategory", "Products", new {id = item.CategoryId})" class="list-group-item list-group-item-action">
            @item.CategoryName
        </a>
    }
</ul>